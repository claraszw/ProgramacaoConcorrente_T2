import java.io.BufferedReader;
import java.io.File;
import java.util.Scanner;


public class Matrix {
	
	public double[][] content;
	int N;
	
	public Matrix(int N){
		this.N=N;
		content = new double[N][N];
	}
	
	public void fillMatrixFile(Scanner source){
		
		for(int i=0;i<N;i++){
			for(int j=0;j<N;j++){
				content[i][j] = source.nextDouble();
			}
		}
	}
	
